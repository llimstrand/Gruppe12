@model dynamic
<head>
    <link rel="stylesheet" href="~/css/add.css" asp-append-version="true" />
</head>

<div class="bakgrunnNyEmpTea">
    <input type="button" value="Tilbake" class= "button-back" onclick="history.go(-1);" /> 

    <form action="/Teams/Lagre" method="post">
            @foreach(var User in Model.Teams){                 
                <input required="required" type="hidden" name="Team_ID" value=@User.Team_ID /> <br>
            }
            
            <li class="NyEmpTeamInputs">
                <p class="medlemTxt">Hvem er med i teamet?</p>
                <select class=input id="DropDownList2" name="Emp_Nr" required> 
                        @foreach (var user in Model.Users){
                            <option value= "@user.Emp_Nr">@user.Emp_Navn</option>
                        }
                </select>
            </li>
            <br><br>
            <input type="submit" value="Legg til medlem" class="sendinn sendinnEmpTea"/>
    </form>
</div>