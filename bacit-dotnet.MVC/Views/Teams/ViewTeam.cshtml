@model bacit_dotnet.MVC.Models.Teams.TeamsViewModel
<style>
    .enkeltteam{
        border: none;
        padding: 50px;
        width: 60%;
        margin: auto;
        border-radius: 10px;
        box-shadow: 5px 10px 18px #888888; 
    }
    .teamknapp {
        width: 130px;
        height: 130px;
        border: 1px solid #878787;
        background-color: white;
    }
    .teamknapper {
        column-count: 2;
        width: 260px;
        column-gap: 0;
        font-size: 10px;
    }
    .button-back {
       font-size: 17px;
       background-color: white;
       border-radius: 20px;
       border: none;
       position:relative;
       margin-left: 90%;
  }
</style>
<div class="enkeltteam"> 
    <input type="button" value="Tilbake" class= "button-back" onclick="history.go(-1);" />

    <h3><b>@Model.Team_ID</b></h3>
    <p> Teamnavn: @Model.Team_Navn Teamleder: @Model.Team_Leder</p>

    <div class="teamknapper">
        <input type="button" value="Se ansattstatistikk"  class="teamknapp">
        <input type="button" value="Bytt team"  class="teamknapp">
        <input type="button" value="Rediger ansatt"  class="teamknapp">
        <input type="button" value="Slett ansatt"  class="teamknapp">
    </div>
</div>